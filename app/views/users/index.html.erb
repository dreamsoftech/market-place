<div class="container">   
  <div class="span8 center">
    <div class="row-fluid">
      <!-- Search Results -->
      <div class="span12">
        <br>
        <%= render "layouts/messages" %>
        <% @users.each do |user| %>
          <div class="row-fluid">
            <div class="booking-blocks">
              <div class="span3 pull-left booking-img text-center">
                <%= image_tag user.profile.photo.url(:medium) %>
                <ul class="unstyled inline">
                   <li><span class="rating-static rating-45"></span></li>
                   <li><i class="icon-star"></i></li>
                   <li><i class="icon-star"></i></li>
                   <li><i class="icon-star"></i></li>
                   <li><i class="icon-star"></i></li>
                   <li><i class="icon-star-empty"></i></li>
                </ul>
              </div>
              <!-- Prepper Profile -->
              <div class="span8" style="overflow:hidden;">
                <h4><%= link_to user.fullname, profile_path(user.profile.token) %></h4>
                <h5>
                  <%= user.profile.title %>
                </h5>
                <p>
                  <% if !user.profile.overview.nil? && user.profile.overview.length > 200 %>
                    <%= user.profile.overview.slice(0, 200) + "... " %>
                    <a href="#">more</a>
                  <% else %>
                    <%= user.profile.overview %>
                  <% end %>
                </p>
              </div>
            </div>
          </div>
        <% end %>
      </div><!--/span8-->
    </div><!--/row-fluid-->
  </div><!-- /span9 -->
</div><!--/container-->